<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/top_layout" android:background="@color/white" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="0">
        <androidx.appcompat.widget.AppCompatImageView android:id="@+id/webviewBackButton" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="18dp" app:srcCompat="@drawable/ic_arrow_back"/>
        <androidx.appcompat.widget.AppCompatTextView android:textColor="@color/primary_purple" android:id="@+id/tv_title" android:padding="0dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="0dp" android:text="@string/alberta_health_webview_title" android:textAllCaps="true" style="@style/OnboardingTitle"/>
    </LinearLayout>
    <WebView android:id="@+id/webview" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/top_layout"/>
</androidx.constraintlayout.widget.ConstraintLayout>
